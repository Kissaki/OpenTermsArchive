{
  "services": {
    "declarationsPath": "../ota-services-france/declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../ota-versions-france",
          "publish": true,
          "prefixMessageToSnapshotId": "This version was recorded after filtering snapshot https://github.com/OpenTermsArchive/snapshots-france/commit/",
          "author": {
            "name": "Open Terms Archive Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    },
    "snapshots": {
      "storage": {
        "mongo": {
          "connectionURI": "mongodb://localhost:27017",
          "database": "open-terms-archive",
          "collection": "snapshots"
        }
      }
    }
  },
  "fetcher": {
    "language": "fr"
  },
  "logger": {
    "sendMailOnError": {
      "to": "admin@opentermsarchive.org",
      "from": "noreply@opentermsarchive.org",
      "sendWarnings": false
    }
  },
  "notifier": {
    "sendInBlue": {
      "updatesListId": 395,
      "updateTemplateId": 60
    }
  },
  "tracker": {
    "githubIssues": {
      "repository": "OpenTermsArchive/declarations-france"
    }
  },
  "dataset": {
    "servicesRepositoryName": "declarations-france",
    "versionsRepositoryURL": "https://github.com/OpenTermsArchive/versions-france"
  }
}
