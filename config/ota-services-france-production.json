{
  "services": {
    "declarationsPath": "../ota-services-france/declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../ota-versions-france",
          "publish": true,
          "prefixMessageToSnapshotId": "This version was recorded after filtering snapshot https://github.com/OpenTermsArchive/snapshots-france/commit/",
          "author": {
            "name": "Open Terms Archive Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    },
    "snapshots": {
      "storage": {
        "git": {
          "path": "../ota-snapshots-france",
          "publish": true,
          "author": {
            "name": "Open Terms Archive Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    }
  },
  "fetcher": {
    "language": "fr"
  },
  "logger": {
    "sendMailOnError": {
      "to": "admin@opentermsarchive.org",
      "from": "noreply@opentermsarchive.org",
      "sendWarnings": false
    }
  },
  "notifier": {
    "sendInBlue": {
      "updatesListId": 191
    }
  },
  "tracker": {
    "githubIssues": {
      "repository": "OpenTermsArchive/declarations-france"
    }
  }
}
