{
  "services": {
    "declarationsPath": "../ota-services-france/declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../ota-versions-france",
          "publish": true,
          "prefixMessageToSnapshotId": "This version was recorded after filtering snapshot with Mongo ID "
        }
      }
    },
    "snapshots": {
      "storage": {
        "path": "../ota-snapshots-france",
        "publish": true
      }
    }
  },
  "fetcher": {
    "language": "fr"
  },
  "logger": {
    "sendMailOnError": {
      "to": "admin@opentermsarchive.org",
      "from": "noreply@opentermsarchive.org",
      "sendWarnings": false
    }
  },
  "notifier": {
    "sendInBlue": {
      "updatesListId": 395,
      "updateTemplateId": 60
    }
  },
  "tracker": {
    "githubIssues": {
      "repository": "OpenTermsArchive/declarations-france"
    }
  },
  "dataset": {
    "servicesRepositoryName": "declarations-france",
    "versionsRepositoryURL": "https://github.com/OpenTermsArchive/versions-france"
  }
}
