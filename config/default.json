{
  "services": {
    "declarationsPath": "../services-all/declarations",
    "documentTypesPath": "../services-all/document-types.json"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "./data/versions",
          "publish": false,
          "publicRepository": "https://github.com/ambanum/OpenTermsArchive-versions.git",
          "snapshotsBaseUrl": "https://github.com/ambanum/OpenTermsArchive-snapshots/commit/",
          "author": {
            "name": "OTA Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    },
    "snapshots": {
      "storage": {
        "git": {
          "path": "./data/snapshots",
          "publish": false,
          "publicRepository": "https://github.com/ambanum/OpenTermsArchive-snapshots.git",
          "author": {
            "name": "OTA Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    }
  },
  "fetcher": {
    "waitForElementsTimeout": 10000,
    "navigationTimeout": 45000
  },
  "logger": {
    "sendMailOnError": false
  },
  "notifier": {
    "sendInBlue": {
      "administratorsListId": 194,
      "updatesListId": 195,
      "updateTemplateId": 21
    }
  }
}
