{
  "services": {
    "declarationsPath": "../ota-declarations-france-elections/declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../ota-versions-france-elections"
        }
      }
    },
    "snapshots": {
      "storage": {
        "git": {
          "path": "../ota-snapshots-france-elections"
        }
      }
    }
  },
  "fetcher": {
