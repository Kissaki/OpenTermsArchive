  },
  "tracker": {
    "githubIssues": {
      "repository": "OpenTermsArchive/declarations-france-elections"
    }
  },
  "dataset": {
    "servicesRepositoryName": "declarations-france-elections",
    "versionsRepositoryURL": "https://github.com/OpenTermsArchive/versions-france-elections"
  }
