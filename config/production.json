{
  "services": {
    "declarationsPath": "../ota-services-all/declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../ota-versions",
          "publish": true,
          "prefixMessageToSnapshotId": "This version was recorded after filtering snapshot https://github.com/ambanum/OpenTermsArchive-snapshots/commit/",
          "author": {
            "name": "Open Terms Archive Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    },
    "snapshots": {
      "storage": {
        "git": {
          "path": "../ota-snapshots",
          "publish": true,
          "author": {
            "name": "Open Terms Archive Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    }
  },
  "logger": {
    "sendMailOnError": {
      "to": "admin@opentermsarchive.org",
      "from": "noreply@opentermsarchive.org",
      "sendWarnings": false
    }
  },
  "notifier": {
    "sendInBlue": {
      "administratorsListId": 9,
      "updatesListId": 191
    }
  }
}
